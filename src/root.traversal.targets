<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <Target Name="Build">
    <MSBuild Projects="@(ProjectFiles)" BuildInParallel="true"  Targets="Build"   Properties="Configuration=$(Configuration)" Condition="'$(TargetPlatform)' == 'coreclr'" />
    <MSBuild Projects="@(ProjectFiles)" BuildInParallel="false" Targets="Build"   Properties="Configuration=$(Configuration)" Condition="'$(TargetPlatform)' != 'coreclr'" />
  </Target>

  <Target Name="Rebuild">
    <MSBuild Projects="@(ProjectFiles)" BuildInParallel="true"  Targets="Rebuild" Properties="Configuration=$(Configuration)" Condition="'$(TargetPlatform)' == 'coreclr'" />
    <MSBuild Projects="@(ProjectFiles)" BuildInParallel="false" Targets="Rebuild" Properties="Configuration=$(Configuration)" Condition="'$(TargetPlatform)' != 'coreclr'" />
  </Target>

  <Target Name="Clean">
    <MSBuild Projects="@(ProjectFiles)" BuildInParallel="false" Targets="Clean"   Properties="Configuration=$(Configuration)" />
  </Target>
</Project>
