    <ItemGroup>
        <Compile Include="test.fs" />
    </ItemGroup>
    <ItemGroup>
      <FscCompilerTools Include="..\..\..\artifacts\bin\Test.Analyzer\Debug\netstandard2.0"/>
    </ItemGroup>

</Project>
