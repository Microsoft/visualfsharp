<Project Sdk="FSharp.NET.Sdk;Microsoft.NET.Sdk" ToolsVersion="15.0">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Library.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="NETStandard.Library" Version="1.6" />
    <PackageReference Include="Microsoft.FSharp.Core.netcore" Version="1.0.0-alpha-161023" />
    <PackageReference Include="FSharp.NET.Sdk" Version="1.0.0-char-*">
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>

    <!-- Override the compiler sdk package, if $(CompilerSdkPackageVersion) is set -->
    <PackageReference 
        Condition=" '$(CompilerSdkPackageVersion)' != '' "
        Include="Microsoft.FSharp.Compiler.Sdk.netcore" Version="$(CompilerSdkPackageVersion)" />

  </ItemGroup>

</Project>
